{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1",
      "status": 200
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "status": 200
    },
    {
      "src": "/dashboard/(.*)",
      "dest": "/dashboard/$1",
      "status": 200
    },
    {
      "src": "/auth/(.*)",
      "dest": "/auth/$1",
      "status": 200
    }
  ]
} 